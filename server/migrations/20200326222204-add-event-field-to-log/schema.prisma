datasource postgresql {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Log {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  // Log
  event   String
  type    LogType
  message String
  data    String?

  // Reference information
  owner      String
  repository String?
}

enum LogType {
  DEBUG INFO WARN
}